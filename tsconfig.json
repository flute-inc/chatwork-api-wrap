{
  "compilerOptions": {
    "target": "es2022",
    "module": "node16",
    "lib": [
      "dom",
      "es2022",
      "scripthost"
    ],
    "declaration": true,
    "declarationDir": "./src/typings",
    "sourceMap": false,
    "outDir": "./src/js",
    "rootDir": "./src/ts",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "baseUrl": "./src/",
    "esModuleInterop": true,
    "moduleResolution": "node16"
  }
}